{
  "name": "@dra2020/topojson-client",
  "version": "3.2.15",
  "description": "Manipulate TopoJSON and convert it to GeoJSON.",
  "keywords": [
    "topojson",
    "topology",
    "geojson"
  ],
  "homepage": "https://github.com/dra2020/topojson-client",
  "license": "ISC",
  "author": {
    "name": "Mike Bostock (fork Terry Crowley)",
    "url": "https://bost.ocks.org/mike"
  },
  "main": "dist/@dra2020/topojson-client.js",
  "types": "src/index.d.ts",
  "unpkg": "dist/@dra2020/topojson-client.min.js",
  "jsdelivr": "dist/@dra2020/topojson-client.min.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dra2020/topojson-client.git"
  },
  "bin": {
    "topo2geo": "bin/topo2geo",
    "topomerge": "bin/topomerge",
    "topoquantize": "bin/topoquantize"
  },
  "files": [
    "bin/topo*",
    "dist/**/*.js",
    "src/**/*.js",
    "src/index.d.ts"
  ],
  "scripts": {
    "pretest": "rollup -c",
    "test": "tape 'test/**/*-test.js' && eslint src test"
  },
  "dependencies": {
    "commander": "2"
  },
  "devDependencies": {
    "eslint": "6",
    "rollup": "1",
    "rollup-plugin-terser": "5",
    "tape": "4"
  }
}
